<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item 
		android:state_focused="true" 
		android:state_pressed="true"
		android:drawable="@drawable/ic_title_barcode_pressed" />
	<item 
		android:state_focused="false" 
		android:state_pressed="true"
		android:drawable="@drawable/ic_title_barcode_pressed" />
	<item 
		android:state_focused="true"
		android:state_enabled="true" 
		android:drawable="@drawable/ic_title_barcode_selected" />
	<item 
		android:state_enabled="false" 
		android:drawable="@drawable/ic_title_barcode_disable" />
	<item 
		android:state_focused="false" 
		android:state_pressed="false"
		android:drawable="@drawable/ic_title_barcode_default" />
    <item
    	android:state_enabled="false"
    	android:drawable="@drawable/ic_title_barcode_disable"/>
</selector>
